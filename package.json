    "build:ts": "npm run clean:win && tsc",
    "build:webpack": "npm run clean:win && webpack",
    "exec:ts": "node --inspect=9230 -r ./tsconfig-paths-bootstrap.js ./dist/Index.js",
    "exec:webpack": "node  ./dist/api.bundle.js",
    "start": "npm run lint && npm run build:webpack && npm run exec:webpack",
    "start:ts": "npm run lint && npm run build:ts && npm run exec:ts",
    "start:dev": "nodemon -e ts --exec \" npm run start:ts \"",
    "test": "mocha  ./test/**/*.spec.js --exit",
    "test-with-coverage": "nyc --reporter=html mocha ./test/**/*.spec.js --exit"
